%div{:class => "navbar navbar-inverse navbar-fixed-top", :role => "navigation"}
	.container
		.navbar-header
			= link_to "Bookster", root_path, class: "navbar-brand"
		.navbar-collapse.collapse.navbar-right
			.nav.navbar-nav
				- if signed_in?
					%li= link_to "All Users", users_path
					%li= link_to "Profile", user_path(current_user)
					%li= link_to "Book Search", search_for_book_path
					%li= link_to "Log Out", session_path(current_user), method: :delete
				- else
					%li= link_to "Log In", login_path	